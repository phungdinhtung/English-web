{
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "printWidth": 120,
  "semi": false,
  "tabWidth": 2,
  "useTabs": true,
  "importOrder": [
    "^(next/(.*)$)|^(next$)",
    "^(react/(.*)$)|^(react$)",
    "^@/pages/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/config/(.*)$",
    "^@/libs/(.*)$",
    "^@/utils/(.*)$",
    "^@/layouts/(.*)$",
    "^@/components/(.*)$",
    "^@/constants/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
